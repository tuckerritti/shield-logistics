{
  "$schema": "https://turbo.build/schema.json",
  "globalEnv": ["SUPABASE_SERVICE_ROLE_KEY"],
  "tasks": {
    "dev": {
      "cache": false,
      "persistent": true
    },
    "build": {
      "dependsOn": ["^build"],
      "cache": false,
      "outputs": [".next/**"]
    },
    "lint": {
      "outputs": []
    },
    "typecheck": {
      "dependsOn": ["^typecheck"],
      "outputs": []
    },
    "test": {
      "dependsOn": ["^test"],
      "outputs": []
    }
  }
}
